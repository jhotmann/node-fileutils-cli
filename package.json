{
  "name": "fileutils-cli",
  "version": "1.0.1",
  "description": "A collection of powerful command line file utilities",
  "main": "src/bin.js",
  "preferGlobal": true,
  "bin": {
    "fileutils": "bin.js",
    "fu": "bin.js"
  },
  "scripts": {
    "test": "jest",
    "build": "pkg --out-path bin -t latest-macos-x64,latest-linux-x64 .",
    "build-win": "pkg --out-path build/bin -t latest-win-x64 .",
    "package-win": "node build/packager-win.js",
    "package-mac": "node build/packager-mac.js"
  },
  "keywords": [
    "cli",
    "file-utils",
    "rename",
    "copy",
    "move"
  ],
  "author": "Jordan Hotmann",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jhotmann/node-fileutils-cli.git"
  },
  "dependencies": {
    "async": "^3.2.0",
    "chalk": "^4.1.0",
    "cli-clear": "^1.0.4",
    "clipboardy": "^2.2.0",
    "date-fns": "^2.16.1",
    "decompress": "^4.2.1",
    "fraction.js": "^4.0.12",
    "fs-extra": "^9.0.1",
    "global-prefix": "^3.0.0",
    "globby": "^11.0.0",
    "got": "^11.8.1",
    "inquirer": "^7.1.0",
    "jpeg-exif": "^1.1.4",
    "mp3tag.js": "^2.2.0",
    "normalize-path": "^3.0.0",
    "nunjucks": "^3.2.2",
    "open": "^7.3.0",
    "path-exists": "^4.0.0",
    "progress": "^2.0.3",
    "readline-sync": "^1.4.10",
    "sequelize": "^6.3.5",
    "sqlite3": "^5.0.0",
    "terminal-kit": "^2.5.1",
    "trammel": "^5.0.0",
    "traverse": "^0.6.6",
    "update-notifier": "^5.0.1",
    "yargs": "^16.2.0"
  },
  "devDependencies": {
    "eslint": "^7.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "jest": "^26.6.3"
  },
  "pkg": {
    "assets": [
      "lib/userData.js",
      "lib/userFilters.js",
      "node_modules/terminal-kit/**/*",
      "node_modules/open/xdg-open",
      "src/**/*"
    ]
  }
}
